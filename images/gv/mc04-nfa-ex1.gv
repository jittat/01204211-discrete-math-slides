digraph finite_state_machine {
	size="4,2"

    rankdir=LR;
    
	node [shape = none]; s [label=""];
	node [shape = doublecircle]; 5;
	node [shape = circle];

	s -> 0;
	0 -> 0 [ label = "0,1" ]
	0 -> 1 [ label = "1" ]
	1 -> 2 [ label = "1" ]
	2 -> 3 [ label = "0" ]
	3 -> 4 [ label = "1" ]
	4 -> 5 [ label = "0" ]
	5 -> 5 [ label = "0,1" ]
	
	0 -> 6 [ label = "0"]
	6 -> 7 [ label = "1"]
	7 -> 8 [ label = "0"]
	8 -> 5 [ label = "1"]

    6 -> 6 [ label = "1"]
    7 -> 7 [ label = "0"]
}

